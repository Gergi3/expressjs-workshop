<h1>Browser</h1>
<form>
    <input type="text" name="search" placeholder="Search..." class="search" value="{{search}}">
    <input type="number" name="from" class="difficulty" placeholder="Difficulty from..." value="{{from}}">
    <span>-</span>
    <input type="number" name="to" class="difficulty" placeholder="Difficulty to..." value="{{to}}">
    <input type="submit" value="search">
</form>

{{#each cubes}}
    <div class="cube">
        <img class="cube" src="{{imageUrl}}" alt="cubeImg">
        <p class="name">{{name}}</p>
        <p><span>Difficulty level:</span> {{difficultyLevel}}</p>
        <a class="btn" href="/cube/details/{{_id}}">Details</a>
    </div>
{{/each}}